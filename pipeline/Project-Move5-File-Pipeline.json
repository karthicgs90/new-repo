{
	"name": "Project-Move5-File-Pipeline",
	"properties": {
		"activities": [
			{
				"name": "FileLookup",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "container"
					},
					{
						"name": "folder"
					},
					{
						"name": "file"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "SourceOutputfeed",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "FileLookup",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"activities": [
						{
							"name": "Wait1",
							"type": "Wait",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 1
							}
						}
					]
				}
			}
		],
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "karthiccontainer"
			},
			"Folder": {
				"type": "string",
				"defaultValue": "outputfeed"
			},
			"File": {
				"type": "string"
			},
			"ArchiveFolder": {
				"type": "string",
				"defaultValue": "archievefeed"
			},
			"ErrorFolder": {
				"type": "string",
				"defaultValue": "errorfeed"
			},
			"dbconnection": {
				"type": "string"
			},
			"dbname": {
				"type": "string"
			},
			"dbtable": {
				"type": "string"
			}
		},
		"folder": {
			"name": "oct16-pipeline"
		},
		"annotations": []
	}
}