{
	"name": "PPcustomersnowflake",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "truncate table Dbo.DimCustomer",
						"disableMetricsCollection": false
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CustomerLakeDS",
						"type": "DatasetReference",
						"parameters": {
							"folder": {
								"value": "@pipeline().parameters.folder",
								"type": "Expression"
							},
							"container": {
								"value": "@pipeline().parameters.container",
								"type": "Expression"
							},
							"file": {
								"value": "@pipeline().parameters.file",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"folder": {
				"type": "string",
				"defaultValue": "inputfeed"
			},
			"container": {
				"type": "string",
				"defaultValue": "karthiccontainer"
			},
			"file": {
				"type": "string",
				"defaultValue": "DimCustomer.csv"
			},
			"domain": {
				"type": "string",
				"defaultValue": "karthic-elastic-pool.database.windows.net"
			},
			"dbname": {
				"type": "string",
				"defaultValue": "karthic-test-cust-db"
			},
			"tablename": {
				"type": "string",
				"defaultValue": "DimCustomer"
			}
		},
		"folder": {
			"name": "snowflakedb"
		},
		"annotations": []
	}
}